<% content_for :title do %>Build Failure Analysis Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="welcome" data-view="Text" data-title="Build Failure Analysis" data-text="" data-moreinfo="Protip: You can drag the widgets around!" style="background-color:#000000;" onclick="location.href='http://localhost:8080';"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="3" data-sizey="1">
      <div data-id="latest_builds" data-view="Daspidlist" data-unordered="true" data-title="Latest builds" data-moreinfo="" style="background-color:#96bf48;"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="common_fault_causes" data-view="List" data-unordered="true" data-title="Common fault causes" data-moreinfo="" ></div>
<br/>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="top_failed_jobs" data-view="Daspidlist" data-unordered="true" data-title="Top failed jobs" data-moreinfo="" style="background-color:#ec663c;"></div>
    </li>

   <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="latest_failed_builds" data-view="Daspidlist" data-title="Latest failed builds" data-unordered="true" style="background-color:#ec663c;"></div>
    </li>

  </ul>
  <center>
      <div data-id="configure_common_causes" data-view="Configurelink" data-title="" data-unordered="true" style="font-size: 14px"></div>
  </center>
</div>


